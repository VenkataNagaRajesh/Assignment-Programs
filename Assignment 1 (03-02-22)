#include <stdio.h>

int main()
{
    int mat1[10][10],mat2[10][10],res_mat[10][10],row,col,i,j,k;    
printf("enter the number of row=");    
scanf("%d",&row);    
printf("enter the number of column=");    
scanf("%d",&col);    
printf("enter the first matrix element=\n");    
for(i=0;i<row;i++)    
{    
for(j=0;j<col;j++)    
{    
scanf("%d",&mat1[i][j]);    
}    
}    
printf("enter the second matrix element=\n");    
for(i=0;i<row;i++)    
{    
for(j=0;j<col;j++)    
{    
scanf("%d",&mat2[i][j]);    
}    
}    
    
printf("The result matrix is=\n");    
for(i=0;i<row;i++)    
{    
for(j=0;j<col;j++)    
{    
res_mat[i][j]=0;    
for(k=0;k<ol;k++)    
{    
res_mat[i][j]+=mat1[i][k]*mat2[k][j];    
}    
}    
}   
for(i=0;i<row;i++)    
{    
for(j=0;j<col;j++)    
{    
printf("%d\t",res_mat[i][j]);    
}    
printf("\n");    
}    
return 0;  
}
